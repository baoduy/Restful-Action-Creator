{
  "name": "restful-action-creator",
  "version": "0.0.1",
  "description": "The helper lib to create the Restful commonicator for SPA.",
  "main": "dist/index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "git-jest": "git submodule update --init --remote configs/jest",
    "clean": "rimraf -f -rmdir dist",
    "build": "npm run clean && tsc",
    "transform": "node package.transform.js",
    "publish:dist": "npm publish dist",
    "publish:whole": "npm publish .",
    "publish:all": "npm-run-all build publish:whole",
    "codecov": "codecov -t",
    "test": "jest --watch --coverage --config=configs/jest/ts.jest.json",
    "test:ci": "jest --ci --coverage --config=configs/jest/ts.jest.json && npm run codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baoduy/Restful-Action-Creator.git"
  },
  "keywords": [
    "Restful",
    "API",
    "Action",
    "Creator"
  ],
  "author": "Duy Hoang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baoduy/Restful-Action-Creator/issues"
  },
  "homepage": "https://github.com/baoduy/Restful-Action-Creator#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.9.4",
    "@types/url-join": "^0.8.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-plugin-source-map-support": "^2.0.1",
    "jest": "^23.5.0",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.9",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "url-join": "^4.0.0"
  },
  "jest": {
    "coverageDirectory": "<rootDir>/tests/__coverage__/",
    "collectCoverage": true
  }
}
